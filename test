 #ifdef _USE_ADX
      // ----------------------------------------------------------------
      v[0].vv[0][0]=i1a_indicatorNumber;
      v[0].vv[0][0]=i1a_period;
      v[0].vv[0][0]=i1a_movingAverage;

      v[0].vv[0][0]=i1b_indicatorNumber;
      v[0].vv[0][0]=i1b_period;
      v[0].vv[0][0]=i1b_movingAverage;
      #endif

      #ifdef _USE_RSI
      // ----------------------------------------------------------------
      v[0].vv[0][0]=i2a_indicatorNumber;
      v[0].vv[0][0]=i2a_period;
      v[0].vv[0][0]=i2a_movingAverage;
      v[0].vv[0][0]=i2a_appliedPrice;

      v[0].vv[0][0]=i2b_indicatorNumber;
      v[0].vv[0][0]=i2b_period;
      v[0].v4[2]=i2b_movingAverage;
      v[0].v4[3]=i2b_appliedPrice;
      #endif

      #ifdef _USE_MFI
      // ----------------------------------------------------------------
      v[0].v5[0]=i3a_indicatorNumber;
      v[0].v5[1]=i3a_period;
      v[0].v5[2]=i3a_movingAverage;

      v[0].v6[0]=i3b_indicatorNumber;
      v[0].v6[1]=i3b_period;
      v[0].v6[2]=i3b_movingAverage;
      #endif
      // ----------------------------------------------------------------

      #ifdef _USE_SAR
      v[0].v7[0]=i4a_indicatorNumber;
      v[0].v7[1]=i4a_stepValue;
      v[0].v7[2]=i4a_maxValue;

      v[0].v8[0]=i4b_indicatorNumber;
      v[0].v8[1]=i4b_stepValue;
      v[0].v8[2]=i4b_maxValue;
      #endif

      #ifdef _USE_ICH
