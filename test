      v[0].v9[2]=i5a_kijunSen;
      v[0].v9[3]=i5a_spanB;

      v[0].v10[0]=i5b_indicatorNumber;
      v[0].v10[1]=i5b_tenkanSen;
      v[0].v10[2]=i5b_kijunSen;
      v[0].v10[3]=i5b_spanB;
      #endif

      #ifdef _USE_RVI
      // ----------------------------------------------------------------
      v[0].v11[0]=i6a_indicatorNumber;
      v[0].v11[1]=i6a_period;
      v[0].v11[2]=i6a_movingAverage;

      v[0].v12[0]=i6b_indicatorNumber;
      v[0].v12[1]=i6b_period;
      v[0].v12[2]=i6b_movingAverage;
      #endif

